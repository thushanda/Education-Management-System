<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="studentContext" prefHeight="267.0" prefWidth="675.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.developerStack.eduManage.controller.StudentFormController">
   <children>
      <Label layoutX="15.0" layoutY="14.0" prefHeight="36.0" prefWidth="202.0" text="New Registration" textFill="#321eb5">
         <font>
            <Font name="Arial Bold" size="19.0" />
         </font>
      </Label>
      <Separator layoutX="14.0" layoutY="54.0" prefHeight="7.0" prefWidth="644.0" />
      <Button layoutX="524.0" layoutY="14.0" mnemonicParsing="false" onAction="#backToHomeOnAction" prefHeight="36.0" prefWidth="114.0" style="-fx-background-color: #141415;" text="Back to home" textFill="WHITE">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <TextField fx:id="txtId" editable="false" layoutX="51.0" layoutY="85.0" prefHeight="25.0" prefWidth="166.0" promptText="Rgistration Code">
         <font>
            <Font name="Arial Bold" size="15.0" />
         </font>
      </TextField>
      <TextField fx:id="txtStudent" layoutX="225.0" layoutY="85.0" prefHeight="27.0" prefWidth="202.0" promptText="Student (search here)">
         <font>
            <Font name="Arial Bold" size="15.0" />
         </font>
      </TextField>
      <Button fx:id="btn" layoutX="472.0" layoutY="192.0" mnemonicParsing="false" onAction="#registeronAction" prefHeight="27.0" prefWidth="166.0" style="-fx-background-color: #0D99ff;" text="Register" textFill="WHITE">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <ComboBox fx:id="cmbProgramRegister" layoutX="441.0" layoutY="85.0" prefHeight="27.0" prefWidth="166.0" promptText="Program" />
      <RadioButton layoutX="51.0" layoutY="153.0" mnemonicParsing="false" text="Paid">
         <font>
            <Font name="Arial Bold" size="13.0" />
         </font>
      </RadioButton>
      <RadioButton layoutX="111.0" layoutY="153.0" mnemonicParsing="false" text="Pending">
         <font>
            <Font name="Arial Bold" size="13.0" />
         </font>
      </RadioButton>
      <Label layoutX="51.0" layoutY="116.0" prefHeight="36.0" prefWidth="105.0" text="Payment Status" textFill="#321eb5">
         <font>
            <Font name="Arial" size="12.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
